[ignore]
<PROJECT_ROOT>/docs/.*
<PROJECT_ROOT>/lib/.*
<PROJECT_ROOT>/coverage/.*
<PROJECT_ROOT>/assets/.*
<PROJECT_ROOT>/test/.*
[include]
/src/*

[libs]

[lints]

all=error
sketchy-null-bool=off
sketchy-null-mixed=off
sketchy-null-number=off
sketchy-null-string=off
unclear-type=off
untyped-import=off
untyped-type-import=off

[options]

suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe

[strict]
